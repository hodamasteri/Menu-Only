class sodaMachine extends VendingMachine
{	
	public int[][] soslots()
	{
		int[][] slot=new int[8][2];
		for(int i=0;i<8;i++)
		{	slot[i][0]=i+1;
			slot[i][1]=10;
		}
		return slot;
	}
	//double[] cost=new int[8];
}
class snackMachine extends VendingMachine
{	
	public int[][] snslots()
	{
		int[][] slot=new int[40][2];
		for(int i=0;i<40;i++)
		{	slot[i][0]=i+1;
			slot[i][1]=10;
		}
		return slot;
	}

	//double[] cost=new int[8];
}
class comboMachine extends VendingMachine
{	
	//int[] snackSlot=new int[20];
	public int[][] cslots()
	{
		int[][] slot=new int[8][2];
		for(int i=0;i<8;i++)
		{	slot[i][0]=i+1;
			slot[i][1]=10;
		}
		return slot;}
	//double[] sodaCost=new int[8];
	//double[] sodaCost=new int[20];
}
